<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="info" name="MyApp">
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %p %c{1}: %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="trace">
			<AppenderRef ref="STDOUT"/>
		</Root>
	</Loggers>
</Configuration>